{
  "name": "@cloak-db/app",
  "version": "0.2.4",
  "description": "Run Cloak DB locally with Docker",
  "type": "module",
  "bin": {
    "cloak-app": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts",
    "dev": "tsup src/cli.ts --format esm --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "cloak-db",
    "database",
    "postgres",
    "docker",
    "cli"
  ],
  "author": "Cloak DB",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloak-DB/cloak-monolith.git",
    "directory": "packages/cli"
  },
  "homepage": "https://www.cloak-db.com",
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2"
  }
}
